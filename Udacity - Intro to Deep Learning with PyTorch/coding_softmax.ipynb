   "outputs": [],
   "source": [
    "def softmax(num):\n",
    "    neg_num = np.negative(num)\n",
    "    exp_num = np.exp(neg_num)\n",
    "    denom = 1 + exp_num\n",
    "    ans = np.reciprocal(denom)\n",
    "    return ans"
   ]
  },
